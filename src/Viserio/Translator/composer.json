{
    "name"       : "viserio/translator",
    "type"       : "library",
    "description": "Narrowspark Framework for Creative People.",
    "keywords"   : ["narrowspark", "viserio", "translator"],
    "license"    : "MIT",
    "homepage"   : "http://github.com/n-Viserio/translator",
    "support"    : {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    },
    "authors"    : [
        {
          "name"    : "Daniel Bannert",
          "email"   : "d.bannert@anolilab.de",
          "homepage": "http://www.anolilab.de",
          "role"    : "Developer"
        }
    ],
    "require": {
        "php"                              : ">=5.6",
        "viserio/contracts"                : "self.version",
        "viserio/filesystem"               : "self.version"
    },
    "require-dev": {
        "viserio/cache"                    : "self.version",
        "symfony/event-dispatcher"         : "~2.8|~3.0",
        "fabpot/php-cs-fixer"              : "~1.10.1",
        "phpunit/phpunit"                  : "~5.0.6"
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Translator\\"        : ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            " Viserio\\Translator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master"                   : "0.10.0-dev"
        }
    },
    "minimum-stability"                    : "dev",
    "prefer-stable"                        : true
}
