{
    "name"       : "viserio/translator",
    "type"       : "library",
    "description": "The Viserio Translator package.",
    "keywords"   : ["narrowspark", "viserio", "translator"],
    "license"    : "MIT",
    "homepage"   : "http://github.com/Narrowspark/translator",
    "support"    : {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    },
    "authors"    : [
        {
          "name"    : "Daniel Bannert",
          "email"   : "d.bannert@anolilab.de",
          "homepage": "http://www.anolilab.de",
          "role"    : "Developer"
        }
    ],
    "require": {
        "php"                               : "^5.6 || ^7.0",
        "viserio/contracts"                 : "self.version",
        "viserio/filesystem"                : "self.version"
        "viserio/support"                   : "self.version"
    },
    "require-dev": {
        "narrowspark/php-cs-fixer-config"   : "^1.1",
        "narrowspark/testing-helper"        : "^1.5",
        "phpunit/phpunit"                   : "^5.1",
        "sepia/po-parser"                   : "^4.2",
        "symfony/event-dispatcher"          : "^3.0 || ^2.8",
        "viserio/cache"                     : "self.version"
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Translator\\"         : ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            " Viserio\\Translator\\Tests\\" : "Tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master"                    : "1.0-dev"
        }
    },
    "suggest": {
        "sepia/po-parser"                   : "Required to use the PO parser (~4.2)."
    },
    "minimum-stability"                     : "dev",
    "prefer-stable"                         : true
}
