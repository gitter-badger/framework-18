{
    "name"       : "viserio/support",
    "type"       : "library",
    "description": "The Viserio Support package.",
    "keywords"   : ["framework", "narrowspark", "support", "viserio", "env", "manager", "dumper"],
    "license"    : "MIT",
    "homepage"   : "http://github.com/narrowspark/framework",
    "support"    : {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    },
    "authors"    : [
        {
          "name"    : "Daniel Bannert",
          "email"   : "d.bannert@anolilab.de",
          "homepage": "http://www.anolilab.de",
          "role"    : "Developer"
        }
    ],
    "require": {
        "php"                                : ">=5.6",
        "ext-mbstring"                       : "*",
        "container-interop/container-interop": "~1.0",
        "danielstjules/stringy"              : "~1.9",
        "ircmaxell/random-lib"               : "~1.1",
        "viserio/contracts"                  : "self.version"
    },
    "require-dev": {
        "cekurte/environment"                : "~0.1",
        "mockery/mockery"                    : "~0.9.3",
        "fabpot/php-cs-fixer"                : "~1.10.1",
        "phpunit/phpunit"                    : "~5.0.6"
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Support\\"             : ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Support\\Tests\\"      : "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master"                     : "0.10.0-dev"
        }
    },
    "suggest": {
        "symfony/var-dumper"                 : "Improves the Helper::dump() (~2.8|~3.0).",
        "cekurte/environment"                : "Required to use the Helper::env() (~0.1)."
    },
    "minimum-stability"                      : "dev",
    "prefer-stable"                          : true
}
