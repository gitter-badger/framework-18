{
    "name"       : "viserio/support",
    "type"       : "library",
    "description": "The Viserio Support package.",
    "keywords"   : ["framework", "narrowspark", "support", "viserio", "manager", "dumper", "log", "logger"],
    "license"    : "MIT",
    "homepage"   : "http://github.com/narrowspark/framework",
    "support"    : {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    },
    "authors"    : [
        {
          "name"    : "Daniel Bannert",
          "email"   : "d.bannert@anolilab.de",
          "homepage": "http://www.anolilab.de",
          "role"    : "Developer"
        }
    ],
    "require": {
        "php"                                 : "^5.6 || ^7.0",
        "danielstjules/stringy"               : "~2.2",
        "ext-mbstring"                        : "*",
        "ircmaxell/random-lib"                : "~1.1",
        "viserio/contracts"                   : "self.version"
    },
    "require-dev": {
        "container-interop/container-interop"                : "~1.1",
        "narrowspark/php-cs-fixer-config"     : "~1.1.5",
        "narrowspark/testing-helper"          : "~1.5",
        "mockery/mockery"                     : "~0.9.3",
        "psr/log"                                            : "~1.0",
        "phpunit/phpunit"                     : "~5.1.3"
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Support\\"                             : ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Support\\Tests\\"                      : "Tests/"
        }
    },
    "provide": {
        "container-interop/container-interop-implementation" : "~1.1",
        "psr/log-implementation"                             : "~1.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master"                                     : "1.0-dev"
        }
    },
    "suggest": {
        "symfony/var-dumper"                                 : "Improves the Dumper::dump() (~2.8|~3.0).",
        "php-di/invoker"                                     : "Required to use the Invoker Class (~1.2).",
        "container-interop/container-interop"                : "Required to use the Invoker Class and ContainerAwareTrait (~1.1).",
        "psr/log"                                            : "Required to use the LoggerAwareTrait (~1.0).",
    },
    "minimum-stability"                                      : "dev",
    "prefer-stable"                                          : true
}
