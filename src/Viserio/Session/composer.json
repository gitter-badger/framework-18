{
    "name"       : "viserio/session",
    "type"       : "library",
    "description": "The Viserio Session package.",
    "keywords"   : ["narrowspark", "viserio", "cookie", "encrypter", "session"],
    "license"    : "MIT",
    "homepage"   : "http://github.com/Narrowspark/session",
    "support"    : {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    },
    "authors"    : [
        {
          "name"    : "Daniel Bannert",
          "email"   : "d.bannert@anolilab.de",
          "homepage": "http://www.anolilab.de",
          "role"    : "Developer"
        }
    ],
    "require": {
        "php"                             : "^7.0.7",
        "ext-session"                     : "*",
        "lcobucci/jwt"                    : "^3.1",
        "viserio/cookie"                  : "self.version",
        "viserio/encrypter"               : "self.version",
        "viserio/support"                 : "self.version"
    },
    "require-dev": {
        "narrowspark/php-cs-fixer-config" : "^1.1",
        "narrowspark/testing-helper"      : "^1.5",
        "phpunit/phpunit"                 : "^5.1"
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Session\\"          : ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Session\\Tests\\"   : "Tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master"                  : "1.0-dev"
        }
    },
    "minimum-stability"                   : "dev",
    "prefer-stable"                       : true
}
