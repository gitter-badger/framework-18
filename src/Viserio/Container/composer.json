{
    "name"       : "viserio/container",
    "type"       : "library",
    "description": "Narrowspark Framework for Creative People.",
    "keywords"   : ["framework", "narrowspark", "viserio", "container", "container-interop", "di", "dependency", "injection", "dependency injection", "definition-interop"],
    "license"    : "MIT",
    "homepage"   : "http://github.com/narrowspark/framework",
    "support"    : {
        "issues": "https://github.com/narrowspark/framework/issues",
        "source": "https://github.com/narrowspark/framework"
    },
    "authors"    : [
        {
          "name"    : "Daniel Bannert",
          "email"   : "d.bannert@anolilab.de",
          "homepage": "http://www.anolilab.de",
          "role"    : "Developer"
        }
    ],
    "require": {
        "php"                                 : ">=5.6",
        "container-interop/container-interop" : "~1.1",
        "container-interop/definition-interop": "0.1.0",
        "PHP-DI/invoker"                      : "~1.2",
        "roave/better-reflection"             : "0.1.0",
        "viserio/support"                     : "self.version"
    },
    "require-dev": {
        "mockery/mockery"                     : "~0.9.3",
        "fabpot/php-cs-fixer"                 : "~1.10.1",
        "phpunit/phpunit"                     : "~5.0.6",
        "league/container"                    : "~2.0.3"
    },
    "autoload": {
        "psr-4": {
            "Viserio\\Container\\"            : ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Viserio\\Container\\Tests\\"     : "Tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master"                      : "0.10.0-dev"
        }
    },
    "minimum-stability"                       : "dev",
    "prefer-stable"                           : true
}
